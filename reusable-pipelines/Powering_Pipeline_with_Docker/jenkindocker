pipeline {
    agent {
        docker {
            image 'ngnix:latest'
            args '-u root'
        }
    }
    
    environment {
      VERSION = '0.1.0'
      RELEASE_VERSION = 'R.2'
    }

    stages {
      stage('Audit tools') {
        steps {
          sh '''
              docker -version
            '''
          sh 'printenv'
          sh 'ls -l $WORKSPACE'
        }
      }
}
}
